<!DOCTYPE html>
<html>
<head>
  <title>Askify</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= yield %>

  <% if user_signed_in? %>
    <p>
      <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
    </p>
  <% end %>
</body>
</html>
